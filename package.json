{
  "name": "electron-builder-react",
  "version": "1.0.0",
  "private": true,
  "description": "electron-builder-react",
  "author": "yhj",
  "main": "main.js",
  "homepage": ".",
  "win": {
    "icon": "public/favicon.ico"
  },
  "dependencies": {
    "connected-react-router": "^6.3.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dist": "electron-builder",
    "package-window": "electron-packager . SCEO2.0 --platform=win32 --arch=x64 --electron-version=1.4.13 --icon=favicon.ico",
    "package-win": "yarn build && electron-builder --win --x64",
    "package-linux": "yarn build && electron-builder --linux",
    "package-mac": "yarn build && electron-builder --mac"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^4.1.1",
    "electron-builder": "^20.38.4",
    "extract-zip": "^1.6.7",
    "yargs-parser": "^13.1.2",
    "typescript": "^3.3.4000"
  },
  "build": {
    "productName": "SCEO2",
    "files": [
      "electron.js"
    ],
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "win": {
      "icon": "public/favicon.ico"
    },
    "extraMetadata": {
      "main": "electron.js"
    }
  }
}
